 func1: {
 #this parameter set will be passed to the function
 #type XXX should correspond to XXXWeightCalc.cxx in the function name
 	  type: FluxHist
	  cv_hist_file: "/pnfs/uboone/persistent/users/rcastill/beamData/data/gsimple_flux.root"
	  #cv_hist_file: "./gsimple_flux.root"
	  rw_hist_file: "/pnfs/uboone/persistent/users/rcastill/beamData/data/redecay_flux.root"
	  #rw_hist_file: "./redecay_flux.root"
          random_seed: 1
          mode: multisim
	  number_of_multisims: 10
 	}
horncurrent:{
        type: FluxUnisim
        CentralValue_hist_file:"/uboone/app/users/jaz8600/work/BeamUncertDev/UnisimHists/may06_10kpot_ntrd1000_flux.root"
        PositiveSystematicVariation_hist_file:"/uboone/app/users/jaz8600/work/BeamUncertDev/UnisimHists/may06_horn175ka_rgen610.6_flux.root"
        NegativeSystematicVariation_hist_file:"/uboone/app/users/jaz8600/work/BeamUncertDev/UnisimHists/may06_horn173ka_rgen610.6_flux.root"
        random_seed:1
        scale_factor_pos:1  
        scale_factor_neg:1
        weight_calculator: "MiniBooNE" # "MicroBooNE" OR "MiniBooNE"
        mode: multisim
        number_of_multisims: 30        
}

